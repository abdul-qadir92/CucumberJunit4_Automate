{
  "server": "hub-cloud.browserstack.com",
  "user": "",
  "key": "",

  "capabilities": {
    "projectName": "BDD",
    "buildName": "CucumberParallel",
    "sessionName": "Offers",
    "debug": "true",
    "networkLogs": "true",
    "seleniumVersion": "4.0.0",
    "consoleLogs": "errors"
  },

  "environments": [
    {
      "browserName": "Chrome",
      "browserVersion": "latest",
      "envOptions": {
        "os": "Windows",
        "osVersion": "11"
      }
    },
    {
      "browserName": "Safari",
      "browserVersion": "15.0",
      "envOptions": {
          "os": "OS X",
          "osVersion": "Monterey"
        }
    },
    {
      "envOptions": {
            "osVersion": "14",
            "deviceName": "iPhone 12",
            "realMobile": "true"
          }
    },
    {
      "envOptions": {
              "osVersion": "14.0",
              "deviceName": "Samsung Galaxy S24",
              "realMobile": "true"
            }
    }
  ]
}